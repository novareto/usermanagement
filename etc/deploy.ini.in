[pipeline:main]
pipeline = gzip session fanstatic usermanagement

[filter:fanstatic]
use = egg:fanstatic#fanstatic
recompute_hashes = false
versioning = true
bottom = true
minified = true
compile = true

[filter:session]
use = egg:cromlech.wsgistate#file_session
session_key = session.key
session_cache = /tmp/ 

[filter:gzip]
use = egg:Paste#gzip

[filter:locale]
use = egg:cromlech.i18n#locale

[app:usermanagement]
use = egg:sqlusers#app
name = sqlusers
dsn = postgresql+psycopg2://kuvb:kuvb@localhost/kuvb
zcml_file = ${zcml:output}
session_key = session.key
langs = de

[server:main]
use = egg:Paste#http
host = 0.0.0.0
port = 7879
